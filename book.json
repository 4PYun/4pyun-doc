{
    "title": "P云开放平台",
    "description": "P云开放平台开发者文档",
    "author": "dev@660pp.com",
    "language": "zh-hans",
    "styles":{
        "website": "/assets/overwrite.css"
    },
    "plugins": [
        "-fontsettings",
        "-sharing",
        "-lunr",
        "-search",
        "-highlight",
        "terminull",
        "prism",
        "prism-themes",
        "code",
        "js-sequence-diagram-full",
        "favicon-absolute",
        "insert-logo",
        "back-to-top-button",
        "edit-link",
        "splitter",
        "chapter-fold",
        "flexible-alerts",
        "bottom-navigation",
        "page-footer-ex",
        "search-pro"
    ],
    "pluginsConfig": {
        "insert-logo": {
            "url": "/assets/logo.png",
            "style": "background: none; height: 40px"
        },
        "edit-link": {
            "base": "https://github.com/4PYun/4pyun-doc/blob/main",
            "label": "编辑该页面"
        },
        "prism": {
            "css": [
                "prism-themes/themes/prism-xonokai.css"
            ],
            "ignore": [
                "sequence",
                "term",
                "vb"
            ]
        },
        "favicon-absolute":{
            "favicon": "/assets/favicon.ico",
            "appleTouchIconPrecomposed152": "/assets/apple-touch-icon-precomposed.png"
        },
        "bottom-navigation": {
            "borderColor": "#00BF70"
        },
        "page-footer-ex": {
            "copyright":"&copy 2022 Shenzhen ChinaRoad Technology Co., Ltd. © All Rights Reserved",
            "update_label": "UPDATED",
            "update_format": "YYYY/MM/DD HH:mm:ss"
        }
    }
}
